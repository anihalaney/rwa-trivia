{
  "name": "trivia",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "prod:start": "ng serve --prod --env=prod",
    "api": "json-server --watch src/db.json",
    "lint": "tslint \"src/**/*.ts\" --project src/tsconfig.json --type-check && tslint \"e2e/**/*.ts\" --project e2e/tsconfig.json --type-check",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "deploy-app-to-firebase": "ng build --dev --env=dev && firebase deploy -P default --only hosting",
    "prod:deploy-app-to-firebase": "ng build  --prod --env=prod && firebase deploy -P production --only hosting",
    "compile-functions": "rmdir /s/q functions\\server & tsc --project functions",
    "compile-scheduler-functions": "rmdir /s/q scheduler\\server & tsc --project scheduler",
    "deploy-functions": "npm run compile-functions && firebase deploy -P default --only functions",
    "prod:deploy-functions": "npm run compile-functions && npm run prod:set-index && firebase deploy -P production --only functions",
    "prod:set-index": "firebase -P production functions:config:set elasticsearch.index.production=true",
    "deploy-functions-local": "npm run compile-functions && firebase serve --only functions",
    "run-scheduler-dev": "npm run compile-scheduler-functions && node scheduler/server/run-scheduler.js dev",
    "run-scheduler-prod": "npm run compile-scheduler-functions && node scheduler/server/run-scheduler.js prod"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/core": "^0.2.0",
    "@angular/animations": "5.2.2",
    "@angular/cdk": "5.1.0",
    "@angular/common": "5.2.2",
    "@angular/compiler": "5.2.2",
    "@angular/core": "5.2.2",
    "@angular/flex-layout": "2.0.0-beta.12",
    "@angular/forms": "5.2.2",
    "@angular/http": "5.2.2",
    "@angular/material": "5.1.0",
    "@angular/platform-browser": "5.2.2",
    "@angular/platform-browser-dynamic": "5.2.2",
    "@angular/router": "5.2.2",
    "@fortawesome/angular-fontawesome": "0.1.0-9",
    "@fortawesome/fontawesome-svg-core": "^1.2.0-11",
    "@fortawesome/free-brands-svg-icons": "^5.1.0-8",
    "@fortawesome/free-solid-svg-icons": "^5.1.0-8",
    "@ngrx/effects": "5.0.1",
    "@ngrx/router-store": "^5.0.1",
    "@ngrx/store": "5.0.0",
    "@ngrx/store-devtools": "5.0.1",
    "@ngx-share/button": "^5.3.1",
    "@ngx-share/core": "^5.3.1",
    "angularfire2": "^5.0.0-rc.6",
    "core-js": "2.5.3",
    "csv": "^2.0.0",
    "dom-to-image": "^2.6.0",
    "firebase": "4.10.1",
    "hammerjs": "2.0.8",
    "html2canvas": "^1.0.0-alpha.12",
    "lodash": "^4.17.5",
    "ngx-img-cropper": "^0.9.7",
    "rxjs": "5.5.6",
    "zone.js": "0.8.20"
  },
  "devDependencies": {
    "@angular/cli": "1.6.6",
    "@angular/compiler-cli": "5.2.2",
    "@types/hammerjs": "2.0.35",
    "@types/jasmine": "2.8.6",
    "@types/node": "7.0.12",
    "codelyzer": "4.1.0",
    "jasmine-core": "2.9.1",
    "jasmine-spec-reporter": "4.2.1",
    "json-server": "0.12.1",
    "karma": "2.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.4.1",
    "karma-jasmine": "1.1.1",
    "karma-jasmine-html-reporter": "0.2.2",
    "protractor": "5.3.0",
    "source-map-explorer": "1.5.0",
    "ts-node": "3.3.0",
    "tslint": "5.9.1",
    "typescript": "2.6.2"
  }
}
