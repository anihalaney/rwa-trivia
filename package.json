{
  "name": "trivia",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "prod:start": "ng serve --prod --configuration=production",
    "api": "json-server --watch src/db.json",
    "lint": "tslint \"src/**/*.ts\" --project src/tsconfig.json --type-check && tslint \"e2e/**/*.ts\" --project e2e/tsconfig.json --type-check",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "deploy-app-to-firebase": "ng build --configuration=dev && firebase deploy -P default --only hosting",
    "prod:deploy-app-to-firebase": "ng build  --prod --configuration=production && firebase deploy -P production --only hosting",
    "compile-functions": "rmdir /s/q functions\\server & tsc --project functions",
    "compile-scheduler-functions": "rmdir /s/q scheduler\\server & tsc --project scheduler",
    "deploy-functions": "npm run compile-functions && firebase deploy -P default --only functions",
    "prod:deploy-functions": "npm run compile-functions && npm run prod:set-index && firebase deploy -P production --only functions",
    "prod:set-index": "firebase -P production functions:config:set elasticsearch.index.production=true",
    "deploy-functions-local": "npm run compile-functions && firebase serve --only functions",
    "run-scheduler-dev": "npm run compile-scheduler-functions && node scheduler/server/run-scheduler.js dev",
    "run-scheduler-prod": "npm run compile-scheduler-functions && node scheduler/server/run-scheduler.js prod",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "serve:ssr": "node dist/server.js",
    "build:client-and-server-bundles": "ng build --prod && ng run trivia:server",
    "webpack:server": "webpack --config webpack.server.config.js"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/core": "^0.6.8",
    "@angular/animations": "6.0.6",
    "@angular/cdk": "6.3.0",
    "@angular/common": "6.0.6",
    "@angular/compiler": "6.0.6",
    "@angular/core": "6.0.6",
    "@angular/flex-layout": "6.0.0-beta.16",
    "@angular/forms": "6.0.6",
    "@angular/http": "6.0.6",
    "@angular/material": "6.3.0",
    "@angular/platform-browser": "6.0.6",
    "@angular/platform-browser-dynamic": "6.0.6",
    "@angular/platform-server": "^6.0.7",
    "@angular/pwa": "^0.6.8",
    "@angular/router": "6.0.6",
    "@fortawesome/angular-fontawesome": "0.1.0-10",
    "@fortawesome/fontawesome-svg-core": "^1.2.0",
    "@fortawesome/free-brands-svg-icons": "^5.1.0",
    "@fortawesome/free-solid-svg-icons": "^5.1.0",
    "@ngrx/effects": "6.0.1",
    "@ngrx/router-store": "6.0.1",
    "@ngrx/store": "6.0.1",
    "@ngrx/store-devtools": "6.0.1",
    "@nguniversal/express-engine": "^6.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^6.0.0",
    "@ngx-share/button": "^6.0.1",
    "@ngx-share/buttons": "^6.0.1",
    "@ngx-share/core": "^6.0.1",
    "angularfire2": "^5.0.0-rc.11",
    "core-js": "2.5.7",
    "dom-to-image": "^2.6.0",
    "domino": "^2.0.2",
    "firebase": "5.1.0",
    "hammerjs": "2.0.8",
    "lodash": "^4.17.10",
    "ngx-img-cropper": "^0.10.4",
    "ngx-papaparse": "^2.1.4",
    "promise-polyfill": "^8.0.0",
    "rxjs": "6.2.1",
    "stream": "0.0.2",
    "zone.js": "0.8.26",
    "@angular/service-worker": "6.0.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular/cli": "6.0.8",
    "@angular/compiler-cli": "6.0.6",
    "@types/hammerjs": "2.0.35",
    "@types/jasmine": "2.8.8",
    "@types/node": "10.3.5",
    "codelyzer": "4.4.1",
    "jasmine-core": "3.1.0",
    "jasmine-spec-reporter": "4.2.1",
    "json-server": "0.14.0",
    "karma": "2.0.4",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "2.0.1",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "1.1.0",
    "protractor": "5.3.0",
    "source-map-explorer": "1.5.0",
    "ts-node": "7.0.0",
    "ts-loader": "^4.2.0",
    "tslint": "5.10.0",
    "typescript": "2.7.2",
    "webpack-cli": "^3.0.8"
  }
}
