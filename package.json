{
  "name": "trivia",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "nativescript": {
    "id": "io.bitwiser.trivia.dev",
    "tns-android": {
      "version": "5.4.0"
    },
    "tns-ios": {
      "version": "5.4.0"
    }
  },
  "scripts": {
    "ng": "ng",
    "start-trivia": "ng serve trivia",
    "start-trivia-admin": "ng serve trivia-admin",
    "prod:start-trivia": "ng serve  trivia --configuration=production",
    "prod:start-trivia-admin": "ng serve  trivia-admin --configuration=production",
    "api": "json-server --watch src/db.json",
    "lint": "tslint \"src/**/*.ts\" --project src/tsconfig.json --type-check && tslint \"e2e/**/*.ts\" --project e2e/tsconfig.json --type-check",
    "test": "jest",
    "test:watch": "jest --watch",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "deploy-functions-local": "npm run compile-functions  && firebase serve --only functions",
    "compile-functions": "rmdir /s/q functions\\server & tsc --project functions",
    "deploy-trivia-functions": "npm run deploy-trivia-functions-build && npm run deploy-functions-app &&  npm run deploy-functions-ssr && npm run deploy-apps-to-firebase && npm install firebase@6.0.2",
    "deploy-trivia-functions-build": "npm run build-trivia:ssr && npm run compile-ssr-functions",
    "build-trivia:ssr": "ng build trivia  --configuration=dev && ng build trivia-admin  --configuration=dev && ng run trivia:server",
    "compile-ssr-functions": "npm run compile-functions && npm run webpack:server && cpx dist/index.html functions/dist && rimraf dist/index.html",
    "webpack:server": "npm install firebase@5.2.0 && webpack --config webpack.server.config.js",
    "deploy-functions-app": "npm run pre-compile-app-functions && firebase deploy -P default --only functions",
    "pre-compile-app-functions": "rimraf functions/index.js &&  cp functions/app-functions.js functions/index.js",
    "deploy-functions-ssr": "npm run pre-compile-ssr-functions && firebase deploy -P default --only functions:ssr",
    "pre-compile-ssr-functions": "rimraf functions/index.js && cp functions/ssr-functions.js functions/index.js",
    "deploy-apps-to-firebase": "firebase deploy -P default --only hosting",
    "prod:deploy-trivia-functions": "npm run prod:deploy-trivia-functions-build && npm run prod:set-index && npm run prod:deploy-functions-app && npm run prod:deploy-functions-ssr && npm run prod:deploy-apps-to-firebase && npm install firebase@6.0.2",
    "prod:deploy-trivia-functions-build": "npm run prod:build-trivia:ssr && npm run compile-ssr-functions",
    "prod:build-trivia:ssr": "ng build trivia  --configuration=production && ng build trivia-admin  --configuration=production && ng run trivia:server",
    "prod:set-index": "firebase -P production functions:config:set environment.production=true",
    "prod:deploy-functions-app": "npm run pre-compile-app-functions && firebase deploy -P production --only functions",
    "prod:deploy-functions-ssr": "npm run pre-compile-ssr-functions && firebase deploy -P production --only functions:ssr",
    "prod:deploy-apps-to-firebase": "firebase deploy -P production --only hosting",
    "run-scheduler-dev": "npm run compile-scheduler-functions && node scheduler/server/run-scheduler.js dev",
    "compile-scheduler-functions": "rmdir /s/q scheduler\\server & tsc --project scheduler",
    "run-scheduler-prod": "npm run compile-scheduler-functions && node scheduler/server/run-scheduler.js prod",
    "dev:android": "tns run android --bundle --env.package_name=io.bitwiser.trivia.dev",
    "dev:ios": "tns run ios --bundle  --env.package_name=io.bitwiser.trivia.dev",
    "prod:android": "tns run android --bundle --env.prod --env.aot --env.uglify --env.package_name=io.bitwiser.trivia",
    "prod:ios": "tns run ios --bundle --env.prod --env.aot --env.uglify --for-device --env.package_name=io.bitwiser.trivia",
    "release:dev:android": "sh release/dev-android.sh",
    "release:dev:ios": "rm -rf platforms/ios && tns prepare ios --bundle --release  --env.aot --env.uglify --for-device --env.package_name=io.bitwiser.trivia.dev",
    "release:prod:android": "sh release/prod-android.sh",
    "release:prod:ios": "rm -rf platforms/ios && tns prepare ios --bundle --release --env.prod --env.aot --env.uglify --for-device --env.package_name=io.bitwiser.trivia"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/core": "~7.2.0",
    "@angular/animations": "~7.2.0",
    "@angular/cdk": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/fire": "^5.1.3",
    "@angular/flex-layout": "^7.0.0-beta.22",
    "@angular/forms": "~7.2.0",
    "@angular/http": "~7.2.0",
